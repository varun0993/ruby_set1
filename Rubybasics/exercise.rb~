# require 'pry'

class Cbd
 def initialize(num1,num2)
 	@num1=num1 * num1
 	@num2=num2 * num2
 	@result=@num1 + @num2
 	puts @num1
 	puts @num1
 	puts @result
 	   result=Array.new()
 	 my_details=Array.new()
 	 my_details << "varun"
 	 my_details << "vviet"
 	 my_details << "Qwinix"
 	 result << my_details
 	 puts result

 end
end

# binding.pry
Cbd.new(5,6)
puts "==================================="
a=gets.to_i
if a%2==0
	puts "even"
else
	puts "odd"
end